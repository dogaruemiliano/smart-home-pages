{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "module": "esnext",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@components/*": ["./components/*"],
      "@constants/*": ["./constants/*"],
      "@models/*": ["./models/*"],
      "@navigation/*": ["./navigation/*"],
      "@screens/*": ["./screen/*"],
      "@services/*": ["./services/*"],
      "@store/*": ["./store/*"],
      "@store": ["./store/index.ts"]
    }
  }
}
